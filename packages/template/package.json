{
  "name": "@vnve/template",
  "version": "1.0.4",
  "description": "vnve's video templates in order to make creating visual novel videos quicker",
  "type": "module",
  "main": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/template.umd.cjs",
    "module": "./dist/template.js",
    "types": "./dist/template.d.ts",
    "exports": {
      ".": {
        "import": "./dist/template.js",
        "require": "./dist/template.umd.cjs",
        "types": "./dist/template.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "rimraf dist && tsc && vite build",
    "prepublishOnly": "npm run build",
    "release": "release-it"
  },
  "keywords": [
    "visual novel",
    "video editor",
    "video synthesizer",
    "video encoder",
    "canvas to video",
    "webcodecs",
    "pixijs",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vnve/core": "workspace:^"
  },
  "peerDependencies": {
    "@vnve/core": "workspace:^"
  }
}
