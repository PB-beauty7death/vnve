{
  "name": "@vnve/core",
  "version": "1.0.0",
  "description": "use pixijs + webcodecs to generate mp4 video, for create some video like visual novel",
  "main": "./dist/core.umd.js",
  "files": ["dist"],
  "module": "./dist/core.js",
  "types": "./dist/core.d.ts",
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "require": "./dist/core.umd.js",
      "types": "./dist/core.d.ts"
    }
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "rimraf dist && tsc && vite build",
    "prepublishOnly": "npm run build",
    "release": "release-it"
  },
  "keywords": [
    "visual novel",
    "video editor",
    "video synthesizer",
    "webcodecs",
    "pixijs"
  ],
  "author": "ForeverSc",
  "license": "MIT",
  "dependencies": {
    "@pixi-essentials/transformer": "^3.0.4",
    "@types/lodash-es": "^4.17.10",
    "@types/uuid": "^9.0.6",
    "gsap": "^3.12.2",
    "lodash-es": "^4.17.21",
    "mp4-muxer": "^2.2.2",
    "pixi.js": "^7.3.2",
    "uuid": "^9.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vnve/vnve"
  }
}
